{
	"author": {
		"email": "Hello@Playform.Cloud",
		"name": "Playform",
		"url": "https://playform.cloud"
	},
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"contributes": {
		"keybindings": [
			{
				"command": "workbench.action.files.openFileFolder",
				"key": "ctrl+alt+o",
				"mac": "alt+cmd+o"
			},
			{
				"command": "workbench.action.closeAllEditors",
				"key": "ctrl+shift+w",
				"mac": "shift+cmd+w"
			},
			{
				"command": "expandLineSelection",
				"key": "ctrl-l",
				"when": "textInputFocus"
			},
			{
				"command": "cursorColumnSelectUp",
				"key": "shift+alt+up",
				"when": "textInputFocus"
			},
			{
				"command": "cursorColumnSelectDown",
				"key": "shift+alt+down",
				"when": "textInputFocus"
			},
			{
				"command": "editor.action.copyLinesDownAction",
				"key": "ctrl+d",
				"mac": "cmd+d",
				"when": "editorTextFocus && !editorReadonly"
			},
			{
				"command": "editor.action.deleteLines",
				"key": "shift+cmd+d",
				"when": "textInputFocus && !editorReadonly"
			},
			{
				"command": "editor.action.moveLinesUpAction",
				"key": "ctrl+shift+up",
				"mac": "cmd+ctrl+up",
				"when": "editorTextFocus && !editorReadonly"
			},
			{
				"command": "editor.action.moveLinesDownAction",
				"key": "ctrl+shift+down",
				"mac": "cmd+ctrl+down",
				"when": "editorTextFocus && !editorReadonly"
			},
			{
				"command": "editor.action.blockComment",
				"key": "ctrl+shift+/",
				"mac": "alt+cmd+/",
				"when": "editorTextFocus && !editorReadonly"
			},
			{
				"command": "editor.action.startFindReplaceAction",
				"key": "ctrl+shift+h",
				"mac": "alt+cmd+shift+f",
				"when": "editorFocus || editorIsOpen"
			},
			{
				"command": "selectAllSearchEditorMatches",
				"key": "alt+f3",
				"mac": "cmd+ctrl+g",
				"when": "inSearchEditor && inSearchEditor"
			},
			{
				"command": "editor.action.startFindReplaceAction",
				"key": "ctrl+h",
				"mac": "alt+cmd+f",
				"when": "editorFocus || editorIsOpen"
			},
			{
				"command": "workbench.action.toggleSidebarVisibility",
				"key": "ctrl+alt+h",
				"mac": "cmd+shift+h"
			},
			{
				"command": "scrollLineUp",
				"key": "ctrl+up",
				"mac": "alt+ctrl+up",
				"when": "textInputFocus"
			},
			{
				"command": "scrollLineDown",
				"key": "ctrl+down",
				"mac": "alt+ctrl+down",
				"when": "textInputFocus"
			},
			{
				"command": "workbench.action.quickOpen",
				"key": "ctrl+shift+o"
			},
			{
				"command": "workbench.action.gotoLine",
				"key": "ctrl-g",
				"mac": "cmd-g"
			},
			{
				"command": "workbench.action.gotoSymbol",
				"key": "ctrl-t",
				"mac": "cmd-t"
			},
			{
				"command": "workbench.action.showAllSymbols",
				"key": "ctrl+shift+t",
				"mac": "cmd+shift+t"
			},
			{
				"command": "editor.action.revealDefinition",
				"key": "ctrl-j",
				"mac": "cmd+j",
				"when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
			},
			{
				"command": "editor.action.marker.next",
				"key": "f8",
				"mac": "cmd+'",
				"when": "editorFocus"
			},
			{
				"command": "workbench.action.nextEditor",
				"key": "ctrl+pagedown"
			},
			{
				"command": "workbench.action.previousEditor",
				"key": "ctrl+pageup"
			},
			{
				"command": "extension.liveServer.goOnline",
				"key": "ctrl+alt+p",
				"mac": "alt+cmd+p"
			}
		]
	},
	"dependencies": {
		"jsonfile": "6.1.0"
	},
	"description": "Popular Brackets keybindings for VS Code.",
	"devDependencies": {
		"typescript-esbuild": "",
		"vscode": "1.0.0"
	},
	"displayName": "Brackets Keymap",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"icon": "brackets-keyboard.png",
	"license": "SEE LICENSE IN LICENSE",
	"name": "brackets-keybindings",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'"
	},
	"type": "module",
	"version": "0.0.1"
}
